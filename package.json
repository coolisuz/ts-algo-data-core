{
  "name": "ts-algo-data-core",
  "version": "1.0.0",
  "description": "A showcase of data structures and algorithms implemented in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "tsx src/index.ts",
    "verify": "yarn format && yarn test && yarn lint && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "./node_modules/.bin/eslint .",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "computer-science",
    "cs",
    "algorithms",
    "data-structures",
    "typescript",
    "javascript",
    "algorithm",
    "javascript-algorithms",
    "typescript-algorithms",
    "sorting-algorithms",
    "graph",
    "tree",
    "interview",
    "interview-preparation"
  ],
  "homepage": "https://github.com/coolisuz/ts-algo-data-core.git",
  "author": "Saidjamol <saidjamol1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
